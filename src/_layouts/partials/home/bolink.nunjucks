{% extends '_layouts/partials/home/slide-wrap.nunjucks' %}

{% set slideId = "bolink" %}
{% set activeMenuItem = "eventEye" %}

{% block slideContent %}
  <div class="dash">
    <div class="logo-wrap">
      <img src="{{ baseUrl }}images/logo/logo-bolink.png" alt="event-eye">
    </div>
  </div>
  <h3 class="margin-top-35">{{ 'bolink.title'|translate }}</h3>
  <div class="margin-top-35">
    <a class="btn btn-simple">
      bolink.no
    </a>
  </div>
{% endblock %}
